import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String str = in.next();
        Set<Character> unique_element = new LinkedHashSet<>();
        for(int i=0;i<str.length();i++){
            char c = str.charAt(i);
            unique_element.add(c);
        }
        for(Character c:unique_element)
            System.out.print(c);
    }
}
